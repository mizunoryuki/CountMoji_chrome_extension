{
    "version": "0.0.1",
    "author": "mizuno_m9",
    "name": "Count Moji",
    "description": "Literally.Count moji.",
    "manifest_version": 3,
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
